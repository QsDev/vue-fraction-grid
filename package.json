{
  "name": "vue-fraction-grid",
  "version": "0.1.0",
  "description": "Flexbox based responsive fraction grid for Vue.js",
  "main": "vue-fraction-grid.js",
  "repository": "git@github.com:bkzl/vue-fraction-grid.git",
  "author": "Bartlomiej Kozal <bkzl@me.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run demo",
    "demo": "vue build demo/index.js",
    "test": "NODE_ENV=production jest"
  },
  "files": [
    "vue-fraction-grid.js",
    "components",
    "utils"
  ],
  "jest": {
    "moduleNameMapper": {
      "components/(.*)$": "<rootDir>/components/$1.vue",
      "utils/(.*)$": "<rootDir>/utils/$1.js",
      "(.*)$": "<rootDir>/$1.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "jest-vue-preprocessor"
    }
  },
  "devDependencies": {
    "jest": "^18.1.0",
    "jest-vue-preprocessor": "^0.1.2",
    "vue-cli": "^2.8.1"
  },
  "dependencies": {
    "reduce-css-calc": "^1.3.0"
  }
}
